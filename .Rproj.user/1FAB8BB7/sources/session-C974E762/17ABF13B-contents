  print("bannana")       
} else if(color=="green"){
  print("Green Apple")  
} else if(color=="red"){
  print("Red Apple")     
} else if(color=="blue"){ 
  print("Lolipop")      
}else if(color=="orange"){
  print("orange")    
}else if(color=="white") 
  {print("milk")            
}else {print("no food")}
